{
  "memory": 256,
  "ttl": 10,
  "timeout": 2,
  "minReplicas": 2,
  "maxReplicas": 4,
  "workers": 1,
  "routes": {
    "status": {
      "path": "/_v/status/:code",
      "public": true
    },
    "catalog": {
      "path": "/_v/catalogs/:level",
      "public": true
    },
    "skus": {
      "path": "/_v/skus/:categoryId",
      "public": true
    },
    "cartOrders": {
      "path": "/_v/cartOrders/:createCart",
      "public": true
    },
    "orders": {
      "path": "/_v/cartPage/:formOrderId",
      "public": true
    },
    "addItems": {
      "path": "/_v/cartItems/:formOrderId",
      "public": true
    },
    "updateItems": {
      "path": "/_v/cartItems/update/:formOrderId",
      "public": true
    },
    "addShippingData": {
      "path": "/_v/cartAttachments/shippingData/:formOrderId",
      "public": true
    },
    "addLogisticAndPaymentData": {
      "path": "/_v/cartAttachments/logisticAndPaymentData/:formOrderId",
      "public": true
    },
    "searchProducts": {
      "path": "/_v/products/search/:productName",
      "public": true
    },
    "searchProductsWithFilter": {
      "path": "/_v/products/search",
      "public": true
    },
    "productSearchComplete": {
      "path": "/_v/products/complete",
      "public": true
    },
    "placeOrderFromExistingCart": {
      "path": "/_v/orderFromExistingCart/:formOrderId",
      "public": true
    }
  }
}
